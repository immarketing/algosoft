window wCheckFltr 'Укажите параметры проверки документов';
  show at  (0,0,100,30);
  tabbedSheet tabbed
    screen  sCheckFltr 'Фильтр';
    show at( , , ,);
    fields
      useFilter     : noprotect;
      dtStart       : noprotect, pickbutton;
      dtFinish      : noprotect, pickbutton;
      fltrfpco.name     : protect , pickbutton;
      sFltrUsDesc       : protect , pickbutton;
        docToCheck  : noprotect;

        BaseDocToCheck: noprotect;
        showUnCor   : noprotect;
buttons
          cmOk,,, 'Отчеты';
          cmCancel,,, 'Отмена';
