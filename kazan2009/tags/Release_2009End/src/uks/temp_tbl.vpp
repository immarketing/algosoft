table struct obj_tbl (
  code		: string,
  vol		: word,
  obj		: comp,
  node		: comp,
  name		: string,
  abbr		: string,
  unit		: string
)
with index (
  obj_tbl01 = obj,
  obj_tbl02 = code,
  obj_tbl03 = node + code
);

table struct plan_tbl2 (
  obj		: comp,
  col		: word,
  per		: word,
  base_val	: double,
  plan_val	: double,
  fact_val	: double,
  
  base_val2	: double,
  plan_val2	: double,
  fact_val2	: double
)
with index (
  plan_tbl01 = obj + col + per
);


table struct myspplan0(
  stage : word,
  cSpPlan : comp
)
;


table struct myplanValue0(
  stage : word,
  cSpPlan : comp,
  iPrice : word,
  iArticle : word,
  val : double,
  col_no_n : word,
  plValNRec : comp
)
;
