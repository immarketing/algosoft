table struct f91_printed
(
  nRec          : comp "",
  cRec          : comp ,
  cnode         : comp,
  printedAtRow  : word,
  code          : String,
  name          : String,
  groupFrom     : word,
  groupTo       : word,
  rowsToGroup   : word,
  extPar        : byte,
  extPar1       : byte,
  isLeaf        : byte,
  sum1          : rep91array,//array [1..3] of double,
  sum2          : rep91array,//array [1..3] of double,
  isKAUGroup    : byte,
  isMakeGroup   : byte,
  coTable       : word
)
with index
(
  f91_ptd_nrec = NREC(Unique, Surrogate),
  f91_ptd_crec = crec,
  f91_ptd_cnode = cnode,
  //f91_ptd_cnode_desc = cnode (desc) + crec (desc),
  f91_ptd_node_prrow = cnode + printedAtRow,
  f91_ptd_isKAUGroup = isKAUGroup,
  f91_ptd_cot_isKAUG = COTABLE+ISKAUGROUP,
  f91_ptd_rowsToGroup = groupFrom + rowsToGroup (desc)

)
;

table struct f91_uniquenodes
(
  cRec          : comp
)
with index
(
  f91_unds_crec = crec (Unique)
  //,
  //f91_unds_crecdesc = crec (desc),
)
;

table struct f91_groupft
(
  cRec          : comp,
  Groupfrom     : word,
  groupTo       : word,
